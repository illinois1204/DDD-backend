{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "local run",
            "command": "npm run local",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Attach Node",
            "processId": "${command:PickProcess}",
            "request": "attach",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        }
    ]
}
